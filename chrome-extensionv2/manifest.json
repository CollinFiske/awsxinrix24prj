{
  "manifest_version": 3,
  "name": "Question Prompt Extension",
  "version": "1.0",
  "description": "Collect redirection links and text from websites.",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}


